/**
 * @param {number[]} nums
 * @return {void} Do not return anything, modify nums in-place instead.
 */
var moveZeroes = function(nums) {
    let left = 0;

    for(let right = 0; curr<nums.length ; curr++){
        if(nums[curr] !== 0){
            [nums[left],nums[right]] = [nums[right], nums[left]];

            left++;
        }
    }




==========>>>  USING TWO POINTERS APPROACH
