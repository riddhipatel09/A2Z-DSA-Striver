Q  https://leetcode.com/problems/merge-sorted-array/submissions/1606560002/?envType=study-plan-v2&envId=top-interview-150


int last = m+n-1;

        while(m>0 && n>0){
            if (nums1[m-1] > nums2[n-1]){
                nums1[last] = nums1[m-1];
                m=m-1;
            }
else{
                nums1[last] = nums2[n-1];
                n = n-1;
            }
            last -= 1;
        }

        while(n>0){
            nums1[last] = nums2[n-1];
            n-=1;
            last-=1;
        }
        
    }
